"\n"
"hock2 : User\n"
"hock2 = MkUser \"hock\" \"not telling\"\n"
msgstr ""
"FromString 用户名 where\n"
"  fromString = MkUserName\n"
"\n"
"FromString 密码 where\n"
"  fromString = MkPassword\n"
"\n"
"hoc2 : 用户\n"
"hock2 = MkUser \"hock\" \"不告诉\"\n"

